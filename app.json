{
  "expo": {
    "newArchEnabled": true,
    "name": "Dolar Ahora",
    "description": "App que muestra el precio de los dolares financieros argentinos y sus variaciones.",
    "slug": "dolar-ahora",
    "version": "1.1.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "dolarahora",
    "orientation": "portrait",
    "backgroundColor": "#000000",
    "primaryColor": "#000000",
    "androidStatusBar": {
      "translucent": true
    },
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png",
      "lang": "es"
    },
    "plugins": [
      [
        "expo-router"
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Virgil.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de5f3556-85a3-4184-b809-0a0ed908e71f",
        "versionCode": 1
      }
    },
    "android": {
      "package": "com.ruloosgaalfras.dolarahora"
    }
  }
}
