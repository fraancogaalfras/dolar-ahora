{
  "expo": {
    "newArchEnabled": true,
    "name": "Dólar Ahora",
    "description": "'Dólar Ahora' te permite conocer al instante el valor de los distintos dólares financieros en Argentina. Además, cuenta con un convertidor integrado para calcular fácilmente montos en distintas cotizaciones. Simple, rápida y siempre actualizada.",
    "slug": "dolar-ahora",
    "version": "3.0.1",
    "platforms": ["android", "web"],
    "scheme": "dolarahora",
    "icon": "./assets/images/icon.png",
    "orientation": "portrait",
    "primaryColor": "#08090e",
    "backgroundColor": "#08090e",
    "userInterfaceStyle": "dark",
    "android": {
      "package": "com.fraancogaalfras.dolarahora",
      "icon": "./assets/images/icon.png"
    },
    "androidStatusBar": {
      "translucent": "true"
    },
    "androidNavigationBar": {
      "backgroundColor": "#08090e"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "lang": "es"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#000000",
          "imageWidth": 200,
          "resizeMode": "cover"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
