{
  "expo": {
    "newArchEnabled": true,
    "name": "DÃ³lar Ahora",
    "description": "App que muestra el precio de los dolares financieros argentinos y sus variaciones.",
    "slug": "dolar-ahora",
    "version": "1.2.0",
    "platforms": ["ios", "android", "web"],
    "scheme": "dolarahora",
    "orientation": "portrait",
    "androidStatusBar": {
      "translucent": true
    },
    "icon": "./assets/images/logo.png",
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/logo.png",
      "lang": "es"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/splash.png",
            "backgroundColor": "#ffffff"
          },
          "imageWidth": 300,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Virgil.otf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de5f3556-85a3-4184-b809-0a0ed908e71f",
        "versionCode": 1
      }
    },
    "android": {
      "package": "com.ruloosgaalfras.dolarahora"
    }
  }
}
