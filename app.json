{
  "expo": {
    "name": "Dolar Now",
    "description": "App que muestra el precio de los dolares financieros argentinos y sus variaciones.",
    "slug": "dolar-ahora",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "backgroundColor": "#000000",
    "primaryColor": "#000000",
    "androidStatusBar": {
      "translucent": true
    },
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png",
      "lang": "es"
    },
    "plugins": [
      ["expo-router"],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Virgil.otf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
